{
  "type": "fine_tuning",
  "info": "EfficientNet_EfficientNet-B2_from_config",
  "description": "Profil z konfiguracji dla modelu: EfficientNet EfficientNet-B2 (plik bazowy: base_A.pt)",
  "data_required": "Profil domyślny",
  "hardware_required": "Standardowy sprzęt",
  "config": {
    "model": {
      "architecture": "EfficientNet",
      "variant": "EfficientNet-B2",
      "input_size": 288,
      "num_classes": 33,
      "pretrained": true,
      "pretrained_weights": "imagenet",
      "feature_extraction_only": false,
      "activation": "relu",
      "dropout_at_inference": false,
      "global_pool": "avg",
      "last_layer_activation": "softmax"
    },
    "training": {
      "epochs": 100,
      "batch_size": 32,
      "learning_rate": 0.00015,
      "optimizer": "AdamW",
      "scheduler": "CosineAnnealingWarmRestarts",
      "num_workers": 4,
      "warmup_epochs": 8,
      "warmup_lr_init": 1e-06,
      "mixed_precision": true,
      "gradient_accumulation_steps": 1,
      "gradient_clip": 1.0,
      "validation_split": 0.2,
      "evaluation_freq": 1,
      "use_ema": false,
      "ema_decay": 1.0,
      "unfreeze_strategy": "unfreeze_all",
      "unfreeze_after_epochs": 5,
      "unfreeze_layers": 15,
      "frozen_lr": 0.0,
      "unfrozen_lr": 0.0
    },
    "regularization": {
      "weight_decay": 0.00015,
      "label_smoothing": 0.1,
      "dropout_rate": 0.3,
      "drop_connect_rate": 0.2,
      "momentum": 0.9,
      "epsilon": 1e-06,
      "swa": {
        "use": true,
        "start_epoch": 60
      },
      "stochastic_depth": {
        "use": false,
        "drop_rate": 0.2,
        "survival_probability": 0.8
      },
      "random_erase": {
        "use": false,
        "probability": 0.25,
        "mode": "pixel"
      }
    },
    "augmentation": {
      "basic": {
        "use": true,
        "rotation": 25,
        "brightness": 0.25,
        "shift": 0.1,
        "zoom": 0.15,
        "horizontal_flip": true,
        "vertical_flip": false
      },
      "mixup": {
        "use": true,
        "alpha": 0.3
      },
      "cutmix": {
        "use": true,
        "alpha": 0.7
      },
      "autoaugment": {
        "use": false
      },
      "randaugment": {
        "use": false,
        "n": 2,
        "m": 9
      },
      "advanced": {
        "contrast": 0.2,
        "saturation": 0.2,
        "hue": 0.1,
        "shear": 0.1,
        "channel_shift": 0.0
      }
    },
    "preprocessing": {
      "normalization": "RGB",
      "resize_mode": "bilinear",
      "cache_dataset": false
    },
    "monitoring": {
      "metrics": {
        "accuracy": true,
        "precision": true,
        "recall": true,
        "f1": true,
        "topk": true,
        "confusion_matrix": true,
        "auc": true
      },
      "logging": {
        "use_tensorboard": true,
        "use_wandb": false,
        "save_to_csv": true,
        "logging_freq": "epoch"
      },
      "visualization": {
        "use_gradcam": true,
        "use_feature_maps": true
      },
      "early_stopping": {
        "patience": 20,
        "min_delta": 0.001,
        "monitor": "val_f1"
      },
      "checkpointing": {
        "best_only": true,
        "save_frequency": 1,
        "metric": "val_f1"
      }
    },
    "advanced": {
      "seed": 42,
      "deterministic": true,
      "class_weights": "balanced",
      "sampler": "weighted_random",
      "image_channels": 3,
      "tta": {
        "use": false,
        "num_augmentations": 5
      },
      "export_onnx": false,
      "quantization": {
        "use": false,
        "precision": "int8"
      },
      "catastrophic_forgetting_prevention": {
        "enable": true,
        "preserve_original_classes": true,
        "rehearsal": {
          "use": true,
          "samples_per_class": 20,
          "synthetic_samples": true
        },
        "knowledge_distillation": {
          "use": true,
          "temperature": 2.0,
          "alpha": 0.4
        },
        "ewc_regularization": {
          "use": true,
          "lambda": 100.0,
          "fisher_sample_size": 200
        },
        "layer_freezing": {
          "strategy": "gradual",
          "freeze_ratio": 0.7
        }
      }
    }
  }
}