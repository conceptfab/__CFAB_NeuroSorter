{
  "type": "fine_tuning",
  "info": "Profil dla klasyfikacji wieloklasowej (5 klas)-FIXED",
  "description": "Profil dostosowany do klasyfikacji około 5 różnych klas-FIXED",
  "data_required": "Zestaw danych zawierający przykłady z około 5 różnych klas-FIXED",
  "hardware_required": "Standardowy sprzęt",
  "config": {
    "model": {
      "architecture": "EfficientNet",
      "variant": "b2",
      "input_size": 260,
      "num_classes": 5,
      "pretrained": true,
      "pretrained_weights": "imagenet",
      "feature_extraction_only": false,
      "activation": "relu",
      "dropout_at_inference": false,
      "global_pool": "avg",
      "last_layer_activation": "softmax"
    },
    "training": {
      "epochs": 100,
      "batch_size": 48,
      "learning_rate": 0.0002,
      "optimizer": "AdamW",
      "scheduler": "CosineAnnealingWarmRestarts",
      "num_workers": 4,
      "warmup_epochs": 5,
      "warmup_lr_init": 5e-7,
      "mixed_precision": true,
      "gradient_accumulation_steps": 2,
      "gradient_clip": 1.0,
      "validation_split": 0.2,
      "evaluation_freq": 1,
      "use_ema": false,
      "ema_decay": 0.9999,
      "unfreeze_strategy": "gradual",
      "unfreeze_after_epochs": 10,
      "unfreeze_layers": 15,
      "frozen_lr": 0.0001,
      "unfrozen_lr": 0.001
    },
    "regularization": {
      "weight_decay": 0.00015,
      "label_smoothing": 0.1,
      "dropout_rate": 0.3,
      "drop_connect_rate": 0.2,
      "momentum": 0.9,
      "epsilon": 1e-6,
      "swa": {
        "use": true,
        "start_epoch": 80
      },
      "stochastic_depth": {
        "use": false,
        "drop_rate": 0.2,
        "survival_probability": 0.8
      },
      "random_erase": {
        "use": false,
        "probability": 0.25,
        "mode": "pixel"
      }
    },
    "augmentation": {
      "image_size": [260, 260],
      "basic": {
        "use": true,
        "rotation": 20,
        "brightness": 0.2,
        "shift": 0.1,
        "zoom": 0.15,
        "horizontal_flip": true,
        "vertical_flip": false
      },
      "mixup": {
        "use": true,
        "alpha": 0.3
      },
      "cutmix": {
        "use": true,
        "alpha": 0.4
      },
      "autoaugment": {
        "use": false
      },
      "randaugment": {
        "use": true,
        "n": 2,
        "m": 7
      },
      "advanced": {
        "contrast": 0.2,
        "saturation": 0.2,
        "hue": 0.1,
        "shear": 0.1,
        "channel_shift": 0.0
      }
    },
    "preprocessing": {
      "image_size": [260, 260],
      "normalization": {
        "mean": [0.485, 0.456, 0.406],
        "std": [0.229, 0.224, 0.225]
      },
      "resize_mode": "bilinear",
      "cache_dataset": false
    },
    "monitoring": {
      "metrics": {
        "accuracy": true,
        "precision": true,
        "recall": true,
        "f1": true,
        "topk": true,
        "confusion_matrix": true,
        "auc": true,
        "balanced_accuracy": true,
        "specificity": true,
        "kappa": true
      },
      "logging": {
        "use_tensorboard": true,
        "use_wandb": false,
        "save_to_csv": true,
        "logging_freq": "epoch"
      },
      "visualization": {
        "use_gradcam": true,
        "use_feature_maps": true,
        "use_pred_samples": true,
        "num_samples": 10
      },
      "early_stopping": {
        "patience": 15,
        "min_delta": 0.0005,
        "monitor": "val_f1"
      },
      "checkpointing": {
        "best_only": true,
        "save_frequency": 1,
        "metric": "val_f1"
      }
    },
    "advanced": {
      "seed": 42,
      "deterministic": true,
      "class_weights": "auto",
      "sampler": "weighted_random",
      "image_channels": 3,
      "tta": {
        "use": true,
        "num_augmentations": 3
      },
      "export_onnx": false,
      "quantization": {
        "use": false,
        "precision": "int8"
      },
      "catastrophic_forgetting_prevention": {
        "enable": true,
        "preserve_original_classes": true,
        "rehearsal": {
          "use": true,
          "samples_per_class": 25,
          "synthetic_samples": true
        },
        "knowledge_distillation": {
          "use": true,
          "temperature": 2.0,
          "alpha": 0.4
        },
        "ewc_regularization": {
          "use": true,
          "lambda": 5000.0,
          "fisher_sample_size": 200,
          "adaptive_lambda": true
        },
        "layer_freezing": {
          "strategy": "gradual",
          "freeze_ratio": 0.7
        }
      },
      "evaluation_on_original_classes": {
        "enable": true,
        "frequency": 5,
        "report_metrics": true
      },
      "model_merging": {
        "enable": false,
        "method": "fisher_weighted",
        "interpolation_factor": 0.5
      }
    }
  }
}
