{
    "type": "training",
    "info": "Profil dla EfficientNet EfficientNet-B0",
    "description": "Profil utworzony przez użytkownika",
    "data_required": "Standardowe dane treningowe",
    "hardware_required": "Standardowy sprzęt",
    "config": {
        "model": {
            "architecture": "EfficientNet",
            "variant": "EfficientNet-B0",
            "input_size": 224,
            "num_classes": 2,
            "pretrained": true,
            "pretrained_weights": "imagenet",
            "feature_extraction_only": false,
            "activation": "swish",
            "dropout_at_inference": false,
            "global_pool": "avg",
            "last_layer_activation": "softmax"
        },
        "training": {
            "epochs": 50,
            "batch_size": 32,
            "learning_rate": 0.001,
            "optimizer": "Adam",
            "scheduler": {
                "type": "None",
                "T_0": 10,
                "T_mult": 2,
                "eta_min": 1e-06
            },
            "num_workers": 4,
            "warmup_epochs": 5,
            "warmup_lr_init": 1e-05,
            "mixed_precision": true,
            "evaluation_freq": 1,
            "use_ema": false,
            "ema_decay": 0.999
        },
        "regularization": {
            "weight_decay": 0.0001,
            "gradient_clip": 1.0,
            "label_smoothing": 0.1,
            "drop_connect_rate": 0.2,
            "dropout_rate": 0.2,
            "momentum": 0.9,
            "epsilon": 1e-06,
            "swa": {
                "use": false,
                "start_epoch": 10,
                "lr_swa": 5e-05
            },
            "stochastic_depth": {
                "use": false,
                "survival_probability": 0.8
            }
        },
        "augmentation": {
            "basic": {
                "use": false,
                "rotation": 30,
                "brightness": 0.2,
                "contrast": 0.2,
                "saturation": 0.2,
                "hue": 0.1,
                "shift": 0.1,
                "zoom": 0.1,
                "horizontal_flip": true,
                "vertical_flip": false
            },
            "mixup": {
                "use": false,
                "alpha": 0.2
            },
            "cutmix": {
                "use": false,
                "alpha": 0.2
            }
        },
        "monitoring": {
            "metrics": {
                "accuracy": false,
                "precision": false,
                "recall": false,
                "f1": false,
                "topk": false,
                "confusion_matrix": false
            },
            "early_stopping": {
                "patience": 10,
                "min_delta": 0.0,
                "monitor": "val_loss"
            },
            "checkpointing": {
                "best_only": false,
                "save_frequency": 1,
                "metric": "val_loss"
            }
        }
    }
}